{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "prefer-template": 0,
    "consistent-return": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "import/no-unresolved": [2, { ignore: ['electron'] }],
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "max-len": ["error", 120]
  },
  "plugins": [
    "import",
    "react"
  ]
}
